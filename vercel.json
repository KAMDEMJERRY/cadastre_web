{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/nextjs"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://cadastre-backend-3whj.onrender.com",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"]
    }
  ]
}